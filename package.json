{
  "name": "placeholder-avatars",
  "version": "1.0.0",
  "description": "Library for generating random svg avatar-like placeholders",
  "main": "./lib/placeholder.js",
  "scripts": {
    "lint": "npx eslint --fix .",
    "build": "babel ./src/placeholder.js -d lib && uglifyjs lib/placeholder.js -o lib/placeholder.min.js -c -m && gzip -c lib/placeholder.min.js > lib/placeholder.min.js.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaichub/placeholder-avatars.git"
  },
  "author": "Nikolai Chub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolaichub/placeholder-avatars/issues"
  },
  "homepage": "https://github.com/nikolaichub/placeholder-avatars#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0"
  },
  "dependencies": {
    "uglify-js": "^3.13.9"
  }
}
